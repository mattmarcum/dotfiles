# This tmux statusbar config was created by tmuxline.vim
# on Tue, 05 May 2015

set -g message-command-style fg="colour27"
set -g message-style fg="colour27"
set -g message-style bg="colour000"
set -g message-command-style bg="colour000"


set -g status "on"
#set -g status-style attr="none"
set -g status-style bg="colour236"
set -g status-justify "left"
set -g status-left-length "100"
#set -g status-left-style attr="none"

set -g status-right-length "100"
#set -g status-right-style attr="none"
set -g status-right-style fg="colour255" 
set -g status-right "< %Y-%m-%d %H:%M < #h"

set -g pane-border-style fg="colour240"
set -g pane-active-border-style fg="colour117"

setw -g window-status-style fg="colour245"
setw -g window-status-style bg="colour236"
#setw -g window-status-style attr="none"
#setw -g window-status-activity-style attr="none"
setw -g window-status-activity-style fg="colour27"
setw -g window-status-activity-style bg="colour236"
setw -g window-status-separator ""
setw -g window-status-format "#I > #W "
setw -g window-status-current-format "#I > #W "
setw -g window-status-current-style fg="colour255"
